{
  "name": "next-blog-template",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "gh-pages": "gh-pages -t -d out",
    "remove:cache": "rm -rf node_modules/.cache",
    "gen:sitemap": "node src/external/gen-sitemap/sitemap",
    "gen:cache": "node src/external/cache/index.js",
    "gen:post": "node src/external/gen-md/index.js",
    "gen:webp": "node src/external/gen-webp/index.js",
    "gen:tour": "node src/external/gen-tourinfo/index.js",
    "deploy": "yarn remove:cache && yarn build && yarn gen:sitemap && yarn gen:cache && yarn export && touch out/.nojekyll",
    "deploy:publish": "yarn deploy && yarn gh-pages"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.4",
    "@tailwindcss/line-clamp": "^0.2.2",
    "google-map-react": "^2.1.10",
    "graphql": "^15.6.1",
    "gray-matter": "^4.0.3",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^7.0.1",
    "react-syntax-highlighter": "^15.4.4",
    "remark-gfm": "^3.0.0",
    "tailwind-scrollbar-hide": "^1.0.3"
  },
  "devDependencies": {
    "@types/google-map-react": "^2.1.7",
    "@types/prettier": "^2.4.2",
    "@types/react": "17.0.29",
    "@types/react-syntax-highlighter": "^15.5.2",
    "autoprefixer": "^10.3.7",
    "axios": "^0.27.2",
    "dotenv": "^14.1.0",
    "eslint": "8.0.0",
    "eslint-config-next": "11.1.2",
    "fast-xml-parser": "^4.0.8",
    "gh-pages": "^3.2.3",
    "globby": "11.0.2",
    "jimp": "^0.16.1",
    "node-public-data-tour-api": "^0.1.1",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "sharp": "^0.30.7",
    "tailwindcss": "^3.1.2",
    "ts-node": "^10.4.0",
    "typescript": "4.4.4"
  }
}
