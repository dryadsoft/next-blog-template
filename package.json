{
  "name": "next-blog-template",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "gh-pages": "gh-pages -t -d out",
    "remove:cache": "rm -rf node_modules/.cache",
    "gen:sitemap": "node src/external/gen-sitemap/sitemap",
    "gen:cache": "node src/external/cache/index.js",
    "gen:post": "node src/external/gen-md/index.js",
    "gen:webp": "node src/external/gen-webp/index.js",
    "gen:tour": "node src/external/gen-tourinfo/index.js",
    "deploy:remove": "yarn remove:cache && yarn build && yarn gen:sitemap && yarn gen:cache && yarn export && touch out/.nojekyll",
    "deploy": "yarn build && yarn gen:sitemap && yarn gen:cache && yarn export && touch out/.nojekyll",
    "deploy:publish": "yarn deploy && yarn gh-pages"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/line-clamp": "^0.4.0",
    "google-map-react": "^2.2.0",
    "gray-matter": "^4.0.3",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.3",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^3.0.1",
    "tailwind-scrollbar-hide": "^1.1.7"
  },
  "devDependencies": {
    "@types/google-map-react": "^2.1.7",
    "@types/prettier": "^2.6.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-syntax-highlighter": "^15.5.3",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "fast-xml-parser": "^4.0.9",
    "gh-pages": "^4.0.0",
    "globby": "11.0.2",
    "jimp": "^0.16.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sharp": "^0.30.7",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
  }
}
